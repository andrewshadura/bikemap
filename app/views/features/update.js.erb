$('#feature-popup-contents').html("<%= escape_javascript(render :file => 'features/show')%>")
$('a[rel*="lightbox"]', $('#feature-popup-contents')).lightBox();
$('#edit-link').click(function(e) {
  $('.feature-popup').load('/features/' + '<%= @feature.id %>' + '/edit');
});
theCustomMap.reloadFeature(<%= @feature.id %>);
